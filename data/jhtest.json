                "WorkflowName": "new workflow",
                "Workflow": {
                    "Work": {
                        "Extract": {
                            "Command": "bin/ahwODBCExtractor",
                            "Parameter": {
                                "DataSource": "DSN=agensgraph",
                                "Initializer": "SET GRAPH_PATH TO network",
                                "Extract": [{
                                    "Name": "auth",
                                    "Query": "select * from auth_roles",
                                    "Sequence": "SEQ123"
                                }]
                            },
                            "Switch": {
                                "Paused": true
                            }
                        },
                        "Load1": {
                            "Command": "bin/ahwPQLoader",
                            "Parameter": {
                                "Loader": "load1 loader",
                                "ConnInfo": "",
                                "Initializer": "SET GRAPH_PATH TO network",
                                "Load": [ {
                                    "Name": "auth",
                                    "Query": "create (:auth_roles_vlabel {uuid:$uuid,name:$name})" 
                                }]
                            }
                        }
                    },
                    "Pipe": [{
                            "Type": "SimpleV1",
                            "Source": {
                                "Work": "Extract",
                                "Pipe": "Out[0]"
                            },
                            "Target": {
                                "Work": "Load1",
                                "Pipe": "In[0]"
                            }
                        }
                    ]
                },
                "Favorite": true
            },
            "res": {
                "Success": true
            }
